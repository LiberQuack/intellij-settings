<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript">
        <added pattern="Array.map(callbackfn,thisArg)" />
        <added pattern="Array.find(predicate,thisArg)" />
        <added pattern="SelectorAPI.find(cssSelector)" />
        <added pattern="SelectorAPI.nth(index)" />
        <added pattern="TestController.click(selector,options)" />
        <added pattern="setTimeout(handler,timeout)" />
        <added pattern="ModelHandlerMixin.fillModel(modelObject,modelTargetField)" />
        <added pattern="ModelHandlerMixin.fillModel(modelObject,modelTargetField,elmProperty)" />
        <added pattern="ModelHandlerMixin.fillModel(property,modelTargetField,elmProperty)" />
        <added pattern="ModelHandlerMixin.fillModel(path,elmProperty)" />
      </blacklist>
      <blacklist language="Java">
        <added pattern="java.io.File.File(pathname)" />
        <added pattern="org.apache.camel.builder.BuilderSupport.simple(value)" />
        <added pattern="org.apache.camel.model.ProcessorDefinition.delay(delay)" />
        <added pattern="org.apache.camel.builder.ValueBuilder.regexReplaceAll(regex,replacement)" />
        <added pattern="org.apache.camel.CamelContext.addComponent(componentName,component)" />
      </blacklist>
    </blacklists>
  </component>
</application>